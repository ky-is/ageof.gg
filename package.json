{
	"name": "ageof.gg",
	"version": "0.1.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"generate:techtree": "tsc -p scripts/tsconfig.json && rm scripts/types.js && rm src/assets/types.js && rm src/models/types.js && node scripts/techTree.js"
	},
	"dependencies": {
		"tailwindcss": "^1.4.6",
		"vue": "^3.0.0-beta.20"
	},
	"devDependencies": {
		"@ky-is/eslint-config": "^2.1.0",
		"@types/node": "^14.0.20",
		"@typescript-eslint/eslint-plugin": "^3.6.0",
		"@typescript-eslint/parser": "^3.6.0",
		"@vue/compiler-sfc": "^3.0.0-beta.20",
		"@vue/eslint-config-typescript": "^5.0.2",
		"autoprefixer": "^9.8.4",
		"eslint": "^7.4.0",
		"eslint-plugin-vue": "^7.0.0-alpha.9",
		"typescript": "^3.9.6",
		"vite": "^1.0.0-beta.11"
	},
	"eslintConfig": {
		"extends": "@ky-is/eslint-config/vue-typescript",
		"rules": {
			"no-unused-vars": "off",
			"vue/comment-directive": "off"
		}
	},
	"eslintIgnore": [
		"dist",
		"scripts/*.js",
		"src/*.js"
	],
	"postcss": {
		"plugins": {
			"tailwindcss": {},
			"autoprefixer": {}
		}
	}
}
