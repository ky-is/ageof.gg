{
	"name": "ageof.gg",
	"version": "0.1.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"generate:techtree": "tsc -p scripts/tsconfig.json && node scripts/techTree.js && rm scripts/techTree.js && rm scripts/types/tree.js && rm src/assets/types.js && rm scripts/effectSummaries.js"
	},
	"dependencies": {
		"tailwindcss": "^1.7.3",
		"vue": "^3.0.0-rc.6",
		"vue-router": "^4.0.0-beta.7"
	},
	"devDependencies": {
		"@ky-is/eslint-config": "^2.1.0",
		"@ky-is/tailwindcss-plugin-width-height": "^1.0.0",
		"@types/node": "^14.6.0",
		"@typescript-eslint/eslint-plugin": "^3.9.1",
		"@typescript-eslint/parser": "^3.9.1",
		"@vue/compiler-sfc": "^3.0.0-rc.6",
		"@vue/eslint-config-typescript": "^5.0.2",
		"autoprefixer": "^9.8.6",
		"eslint": "^7.7.0",
		"eslint-plugin-vue": "^7.0.0-beta.2",
		"postcss-preset-env": "^6.7.0",
		"typescript": "^3.9.7",
		"vite": "^1.0.0-rc.4"
	},
	"eslintConfig": {
		"extends": "@ky-is/eslint-config/vue-typescript",
		"rules": {
			"no-unused-vars": "off",
			"vue/comment-directive": "off",
			"vue/no-template-key": "off"
		}
	},
	"eslintIgnore": [
		"dist",
		"scripts/*.js",
		"src/*.js"
	],
	"postcss": {
		"plugins": {
			"postcss-preset-env": {
				"stage": 0
			},
			"tailwindcss": {},
			"autoprefixer": {}
		}
	}
}
